  "require": {
    "tightenco/collect": "^8.81",
    "nesbot/carbon": "^2.32.2",
    "symfony/string": "^6.0",
    "phpstan/phpstan": "^0.12.65",
    "php-ds/php-ds": "^1.4",
    "fzaninotto/faker": "^1.5",
    "knplabs/github-api": "^3.0",
