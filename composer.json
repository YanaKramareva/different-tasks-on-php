    "symfony/var-dumper": "^4.0",
    "phpunit/phpunit": "^9.1.3",
    "squizlabs/php_codesniffer": "^3.5.5",
    "hexlet/trees": "v1.0.1",
    "hexlet/html-tags": "v1.0.1",
    "hexlet/pairs": "v1.0.1",
    "hexlet/pairs-data": "v1.0.1",
    "mikey179/vfsstream": "v1.6.10"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
