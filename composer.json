{
  "name": "hexlet/php-package",
  "bin": ["bin/php-package"],
  "type": "library",
  "authors": [
    {
      "name": "Yana Kramareva",
      "email": "y.kramareva@gmail.com"
    }
  ],
  "description": "php-package",
  "license": "MIT",
  "scripts": {
    "test": "phpunit tests"
  },
  "autoload": {
    "psr-4": {
      "Php\\Package\\": "src",
      "Php\\Package\\Tests\\": "tests",
      "App\\": "src/"
    },
    "files": [
      "src/Utils.php",
      "src/Convert.php",
      "src/findFilesByName.php",
      "src/flatten.php",
      "src/map_filter_reduce.php",
      "src/select.php",
      "src/stringify.php",
      "src/listFlatten.php",
      "src/sameParityFilter.php",
      "src/stringify.php",
      "src/factorial.php",
      "src/sum.php",
      "src/product.php",
      "src/factor.php",
      "src/Double.php",
      "src/cons_car_cdr.php",
      "src/Rational.php",
      "src/Lenth.php",
      "src/Append.php",
      "src/Reverse.php",
      "src/Url.php",
      "src/Safe.php",
      "src/Converter.php"
    ]
  },
  "require": {
    "tightenco/collect": "^5.6",
    "nesbot/carbon": "^2.32.2",
    "symfony/string": "^5.1",
    "phpstan/phpstan": "^0.12.65"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "phpstan/phpstan-phpunit": "^0.12.16",
    "phpstan/extension-installer": "^1.0",
    "symfony/var-dumper": "^4.0",
    "phpunit/phpunit": "^9.1.3",
    "squizlabs/php_codesniffer": "^3.5.5",
    "hexlet/trees": "v1.0.1",
    "hexlet/html-tags": "v1.0.1",
    "hexlet/pairs": "v1.0.1",
    "hexlet/pairs-data": "v1.0.1"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
