{
  "name": "hexlet/php-package",
  "bin": ["bin/php-package"],
  "type": "library",
  "authors": [
    {
      "name": "Yana Kramareva",
      "email": "y.kramareva@gmail.com"
    }
  ],
  "description": "php-package",
  "license": "MIT",
  "scripts": {
    "test": "phpunit tests"
  },
  "autoload": {
    "psr-4": {
      "Php\\Package\\": "src",
      "Php\\Package\\Tests\\": "tests",
      "App\\": "src"
    },
    "files": [
      "src/Utils.php",
      "src/Convert.php",
      "src/FindFilesByName.php",
      "src/Flatten.php",
      "src/Map.php",
      "src/Select.php",
      "src/Stringify.php",
      "src/SameParityFilter.php",
      "src/Stringify.php",
      "src/Factorial.php",
      "src/Sum.php",
      "src/Product.php",
      "src/Factor.php",
      "src/Double.php",
      "src/ConsCarCdr.php",
      "src/Rational.php",
      "src/Lenth.php",
      "src/Append.php",
      "src/Reverse.php",
      "src/Url.php",
      "src/Safe.php",
      "src/Converter.php",
      "src/Comparator.php",
      "src/Arrays.php",
      "src/Normalizer.php",
      "src/GetQuestions.php"
    ]
  },
  "require": {
    "tightenco/collect": "^8.81",
    "nesbot/carbon": "^2.32.2",
    "symfony/string": "^6.0",
    "phpstan/phpstan": "^0.12.65",
    "php-ds/php-ds": "^1.4"
  },
  "require-dev": {
    "psy/psysh": "@stable",
    "phpstan/phpstan-phpunit": "^0.12.16",
    "phpstan/extension-installer": "^1.0",
    "symfony/var-dumper": "^4.0",
    "phpunit/phpunit": "^9.1.3",
    "squizlabs/php_codesniffer": "^3.5.5",
    "hexlet/trees": "v1.0.1",
    "hexlet/html-tags": "v1.0.1",
    "hexlet/pairs": "v1.0.1",
    "hexlet/pairs-data": "v1.0.1"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
